{
  "name": "@project-starter/nuxt",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxi dev --host --fork",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev --host",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview --host",
    "preview:build": "pnpm build && pnpm preview --host",
    "preview:generate": "pnpm generate && pnpm preview --host",
    "typecheck": "nuxi typecheck",
    "clear": "pnpx rimraf .nuxt .output dist",
    "postinstall": "nuxi prepare"
  },
  "devDependencies": {
    "@hebilicious/vue-query-nuxt": "catalog:frontend-nuxt",
    "@pinia/nuxt": "catalog:frontend-nuxt",
    "@project-starter/eslint-config": "workspace:*",
    "@project-starter/nuxt-layer-primo": "workspace:*",
    "@project-starter/unocss-config": "workspace:*",
    "@tanstack/vue-query": "catalog:frontend-vue",
    "@tanstack/vue-query-devtools": "catalog:dev",
    "@unocss/reset": "catalog:frontend",
    "@vite-pwa/nuxt": "catalog:frontend-nuxt",
    "nuxt": "catalog:frontend-nuxt",
    "pinia": "catalog:frontend-vue",
    "typescript": "catalog:dev",
    "unocss": "catalog:frontend",
    "vue-tsc": "catalog:dev"
  }
}
