{
  "name": "@project-starter/nuxt",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxi dev --host --fork",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev --host",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview --host",
    "preview:build": "pnpm build && pnpm preview --host",
    "preview:generate": "pnpm generate && pnpm preview --host",
    "check-types": "nuxi typecheck",
    "clear": "rimraf .nuxt; rimraf .output; rimraf dist || true",
    "postinstall": "nuxi prepare"
  },
  "devDependencies": {
    "@hebilicious/vue-query-nuxt": "catalog:",
    "@pinia/nuxt": "catalog:",
    "@project-starter/eslint-config": "workspace:*",
    "@project-starter/nuxt-layer-primo": "workspace:*",
    "@project-starter/unocss-config": "workspace:*",
    "@tanstack/vue-query": "catalog:",
    "@tanstack/vue-query-devtools": "catalog:",
    "@unocss/reset": "catalog:",
    "@vite-pwa/nuxt": "catalog:",
    "nuxt": "catalog:",
    "pinia": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "vue-tsc": "catalog:"
  }
}
