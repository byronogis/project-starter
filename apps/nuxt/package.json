{
  "name": "@project-starter/nuxt",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxi dev --host --fork",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev --host",
    "build": "nuxi build",
    "generate": "nuxi generate",
    "preview": "nuxi preview --host",
    "preview:build": "pnpm build && pnpm preview --host",
    "preview:generate": "pnpm generate && pnpm preview --host",
    "check-types": "nuxi typecheck",
    "clear": "rm -rf .nuxt .output",
    "postinstall": "nuxi prepare"
  },
  "devDependencies": {
    "@hebilicious/vue-query-nuxt": "^0.3.0",
    "@nuxt/devtools": "^1.6.1",
    "@nuxt/eslint": "^0.7.2",
    "@nuxt/icon": "^1.8.2",
    "@pinia/nuxt": "^0.7.0",
    "@project-starter/eslint-config": "workspace:*",
    "@project-starter/nuxt-layer-sakai": "workspace:*",
    "@project-starter/shared": "workspace:*",
    "@project-starter/unocss-config": "workspace:*",
    "@tanstack/vue-query": "^5.61.3",
    "@tanstack/vue-query-devtools": "^5.61.3",
    "@unocss/nuxt": "^0.64.1",
    "@unocss/reset": "^0.64.1",
    "@vite-pwa/nuxt": "^0.10.6",
    "nuxt": "^3.14.1592",
    "pinia": "^2.2.6",
    "postcss-mixins": "^11.0.3",
    "postcss-nesting": "^13.0.1",
    "typescript": "^5.7.2",
    "unocss": "^0.64.1",
    "vue-tsc": "^2.1.10"
  }
}
