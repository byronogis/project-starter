      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./base": {
      "types": "./dist/config/base.d.ts",
      "import": "./dist/config/base.mjs",
      "require": "./dist/config/base.cjs"
    }
